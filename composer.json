{
  "name": "seatplus/srp",
  "type": "seatplus-plugin",
  "description": "description",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Felix Huber",
      "email": "felix.a.huber@gmx.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seatplus\\Srp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seatplus\\Srp\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-redis": "*",
    "laravel/framework": "^8.0",
    "seatplus/web": "^0.9.2"
  },
  "require-dev": {
    "orchestra/testbench": "^6.10",
    "nunomaduro/collision": "^5.3",
    "spatie/phpunit-watcher": "^1.11",
    "rector/rector": "^0.9.2",
    "brianium/paratest": "^6.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seatplus\\Srp\\SrpServiceProvider"
      ]
    }
  },
  "scripts": {
    "watch": "vendor/bin/phpunit-watcher watch",
    "parallel": "vendor/bin/testbench package:test --parallel --no-coverage"
  }
}